{
  "expo": {
    "newArchEnabled": true,
    "name": "Doki",
    "slug": "doki",
    "scheme": "com.app.doki",
    "owner": "kennethprz09",
    "extra": {
      "eas": {
        "projectId": "439410ef-8376-4395-9906-0a49a644fedc"
      }
    },
    "android": {
      "package": "com.app.doki",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.app.doki",
      "iCloudContainerEnvironment": "Production",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Permitir el acceso a la cámara para escanear documentos",
        "NSPhotoLibraryUsageDescription": "Permitir el acceso a la galería para seleccionar imágenes",
        "NSPhotoLibraryAddUsageDescription": "Permitir guardar imágenes en la galería"
      }
    },
    "plugins": [
      "expo-font",
      "expo-document-picker",
      "expo-camera",
      "expo-file-system"
    ]
  }
}
